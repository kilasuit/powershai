<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
	<ViewDefinitions>
		<View>
			<Name>ChatList</Name>
			<ViewSelectedBy>
				<TypeName>PowershaiType:Chat</TypeName>
			</ViewSelectedBy>
			<CustomControl>
				<CustomEntries>
				  <CustomEntry>
					<CustomItem>
						<Frame>
						  <LeftIndent>0</LeftIndent>
						  <RightIndent>0</RightIndent>
						  <FirstLineIndent>0</FirstLineIndent>
						  <CustomItem>
								<ExpressionBinding>
									<ScriptBlock>
										return $null;
									</ScriptBlock>
								</ExpressionBinding>
						  </CustomItem>
						</Frame>
					</CustomItem>
				  </CustomEntry>
				</CustomEntries>
		  </CustomControl>
		</View>
	</ViewDefinitions>
</Configuration>